{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "robotframework-lsp",
            "name": "Robot Framework: Launch template",
            "request": "launch",
            "terminal": "integrated",
            "cwd": "${workspaceFolder}",
            "target": "${file}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}${pathSeparator}${env:PYTHONPATH}"
            },
            "args": [
                "--argumentfile",
                "${workspaceFolder}/robot_arguments.txt"
            ]
        }
    ]
}